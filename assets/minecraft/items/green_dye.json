{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/green_dye"
        },
        "cases": [
            {
                "when": "treasureshop_tile_white",
                "model": {
                    "type": "model",
                    "model": "item/treasureshop_tile_white"
                }
            },
            {
                "when": "treasureshop_tile_black",
                "model": {
                    "type": "model",
                    "model": "item/treasureshop_tile_black"
                }
            }
        ]
    }
}